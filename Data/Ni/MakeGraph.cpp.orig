#include <iostream>

#include <TFile.h>
#include <TH1.h>


void MakeGraph()
{
   TFile *file = new TFile("distance0040.root", "OPEN");

   for(Int_t i = 1; i <=3750; i++){
      TString hisName = Format("HisDose%04d", i);
      TH1D *His = (TH1D*)file->Get(hisName);
      cout << His->GetMean() << endl;
   }
}

